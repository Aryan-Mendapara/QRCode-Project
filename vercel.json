{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "server/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/project/qrcode/(.*)",
      "dest": "/server/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
